{
  "usesDtcg": true,
  "source": ["design-tokens/merged-tokens.json"],
  "platforms": {
    "css": {
      "transformGroup": "css",
      "buildPath": "styles/",
      "files": [
        {
          "destination": "variables.css",
          "format": "css/variables"
        }
      ]
    },
    "scss": {
      "transformGroup": "scss",
      "buildPath": "styles/",
      "files": [
        {
          "destination": "_variables.scss",
          "format": "scss/variables"
        }
      ]
    }
  }
}
