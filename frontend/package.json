    "@pinia/nuxt": "^0.11.1",
    "@unhead/vue": "^2.0.11",
    "nuxt": "^3.17.6",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "vuetify-nuxt-module": "0.18.7"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.5.0",
    "@nuxt/eslint": "1.5.2",
