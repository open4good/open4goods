{
  "name": "nudger-nuxt-front",
  "private": true,
  "type": "module",
  "scripts": {
    "full": "pnpm install && pnpm prepare:nuxt && pnpm generate:api && pnpm build && pnpm generate && pnpm lint && pnpm test --run",
    "fullmem": "NODE_OPTIONS='--max-old-space-size=8192' pnpm install && pnpm generate:api && pnpm build && pnpm generate && pnpm lint && pnpm test --run",
    "dev": "nuxt dev",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "generate": "nuxt generate",
    "lint": "eslint . && pnpm lint:forbidden",
    "lint:forbidden": "node scripts/lint-forbidden.js",
    "lint:fix": "eslint . --fix && prettier --write .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage.enabled true --coverage.reporter=lcov --coverage.reportsDirectory=artifacts/coverage",
    "test:visual": "playwright test",
    "prepare:nuxt": "nuxt prepare",
    "generate:api": "openapi-generator-cli generate -i https://beta.front-api.nudger.fr/v3/api-docs/front -g typescript-fetch -t ./config/openapi-templates -o shared/api-client --skip-validate-spec",
    "generate:api-local": "openapi-generator-cli generate -i http://localhost:8086/v3/api-docs/front -g typescript-fetch -t ./config/openapi-templates -o shared/api-client --skip-validate-spec",
    "semantic-release": "semantic-release",
    "postinstall": "pnpm prepare:nuxt && pnpm rebuild sharp",
    "prepare": "husky",
    "fast": "pnpm lint && pnpm test --run && pnpm build",
    "mcp:vuetify": "npx @vuetify/mcp"
  },
  "dependencies": {
    "@capacitor/core": "^8.0.0",
    "@hcaptcha/vue3-hcaptcha": "^1.3.0",
    "@mdi/js": "^7.4.47",
    "@nuxt/content": "^3.5.0",
    "@nuxt/icon": "2.2.1",
    "@nuxt/image": "2.0.0",
    "@pinia/nuxt": "^0.11.2",
    "@unhead/vue": "^2.0.14",
    "@vueuse/core": "^14.0.0",
    "@vueuse/nuxt": "^14.0.0",
    "better-sqlite3": "^12.6.0",
    "cookie-es": "^2.0.0",
    "date-fns": "^4.0.0",
    "echarts": "^6.0.0",
    "highlight.js": "^11.10.0",
    "isomorphic-dompurify": "^2.28.0",
    "jsdom": "^28.0.0",
    "jwt-decode": "^4.0.0",
    "lz-string": "^1.5.0",
    "markdown-it": "^14.1.0",
    "minisearch": "^7.1.2",
    "nuxt": "^4.0.0",
    "pinia": "^3.0.3",
    "sharp": "^0.34.0",
    "unhead": "^2.0.14",
    "vue": "3.5.28",
    "vue-barcode-reader": "^1.0.3",
    "vue-echarts": "^8.0.0",
    "vue-hotjar": "^1.4.0",
    "vue-pdf-embed": "^2.1.3",
    "vue3-picture-swipe": "^3.0.6",
    "vuetify-nuxt-module": "^0.19.0"
  },
  "devDependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/cli": "^8.0.0",
    "@capacitor/ios": "^8.0.0",
    "@iconify-json/mdi": "^1.2.3",
    "@nuxt/devtools": "^3.0.0",
    "@nuxt/eslint": "1.15.1",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/scripts": "0.13.2",
    "@nuxt/test-utils": "3.23.0",
    "@nuxtjs/device": "^4.0.0",
    "@nuxtjs/i18n": "10.2.3",
    "@nuxtjs/plausible": "^2.0.0",
    "@nuxtjs/robots": "^5.5.1",
    "@nuxtjs/seo": "^3.1.0",
    "@nuxtjs/sitemap": "^7.5.2",
    "@openapitools/openapi-generator-cli": "^2.23.1",
    "@playwright/test": "^1.57.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.0",
    "@semantic-release/npm": "^13.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/lz-string": "^1.5.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vite-pwa/nuxt": "^1.0.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vue/compiler-dom": "3.5.28",
    "@vue/test-utils": "^2.4.6",
    "@vuetify/mcp": "^0.5.0",
    "@vueuse/nuxt": "^14.0.0",
    "dayjs-nuxt": "^2.1.11",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.4.0",
    "happy-dom": "^20.0.0",
    "husky": "^9.1.7",
    "nuxt-site-config": "^3.2.14",
    "prettier": "^3.6.2",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.92.1",
    "semantic-release": "^25.0.0",
    "typescript": "^5.6.3",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vite-plugin-vue-mcp": "^0.3.2",
    "vite-plugin-vuetify": "^2.1.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.6",
    "vuetify": "^3.9.7"
  },
  "packageManager": "pnpm@10.29.3",
  "engines": {
    "node": ">=20.x"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@openapitools/openapi-generator-cli",
      "@parcel/watcher",
      "better-sqlite3",
      "esbuild",
