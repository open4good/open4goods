      "unrs-resolver"
    ],
    "overrides": {
      "glob": "^13.0.0",
      "tar": "^7.5.4",
      "lodash": "^4.17.23",
      "lodash-es": "^4.17.23",
      "undici": "^7.0.0"
    }
  }
