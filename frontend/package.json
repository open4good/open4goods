    "@storybook/vue3": "^8.6.14",
    "@typescript-eslint/parser": "^8.34.0",
    "@vite-pwa/nuxt": "^1.0.4",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/nuxt": "^13.3.0",
    "eslint": "^9.30.1",
