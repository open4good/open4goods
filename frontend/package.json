{
  "name": "nudger-nuxt-front-v2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "build:ssr": "NODE_OPTIONS='--max-old-space-size=8192' nuxt build",
