{
  "name": "nudger-nuxt-front",
  "private": true,
  "type": "module",
  "scripts": {
    "full": "pnpm install && pnpm generate:api && pnpm build && pnpm generate && pnpm lint && pnpm test --run",
    "dev": "nuxt dev",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "generate": "nuxt generate",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix && prettier --write .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "generate:api": "openapi-generator-cli generate -i https://beta.front-api.nudger.fr/v3/api-docs/front -g typescript-fetch -t ./config/openapi-templates -o shared/api-client --skip-validate-spec",
    "preprocess:css": "node ./scripts/preprocess-css.mjs",
    "semantic-release": "semantic-release",
    "postinstall": "nuxt prepare && pnpm rebuild sharp || true",
    "prebuild": "pnpm preprocess:css",
    "prepare": "husky"
  },
  "dependencies": {
    "sharp": "^0.34.0",
    "@hcaptcha/vue3-hcaptcha": "^1.3.0",
    "@mdi/font": "^7.4.47",
    "@nuxt/fonts": "0.11.4",
    "@nuxt/icon": "2.0.0",
    "@nuxt/image": "1.11.0",
    "@pinia/nuxt": "^0.11.2",
    "@unhead/vue": "^2.0.14",
    "@vuetify/mcp": "^0.1.1",
    "@vueuse/core": "^13.9.0",
    "@vueuse/nuxt": "^13.9.0",
    "cookie-es": "^2.0.0",
    "isomorphic-dompurify": "^2.28.0",
    "i": "^0.3.7",
    "jsdom": "^27.0.0",
    "jwt-decode": "^4.0.0",
    "npm": "^11.6.0",
    "nuxt": "^4.0.0",
    "nuxt-device": "^2.1.0",
    "nuxt-mcp": "^0.2.4",
    "pinia": "^3.0.3",
    "unhead": "^2.0.14",
    "vue": "3.5.22",
    "vuetify-nuxt-module": "^0.18.7"
  },
  "devDependencies": {
    "@iconify-json/bxl": "^1.2.2",
    "@iconify-json/mdi": "^1.2.3",
    "@nuxt/devtools": "^2.6.3",
    "@nuxt/eslint": "1.9.0",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/scripts": "0.12.1",
    "@nuxt/test-utils": "3.19.2",
    "@nuxtjs/i18n": "10.1.0",
    "@nuxtjs/plausible": "^2.0.0",
    "@nuxtjs/robots": "^5.5.1",
    "@nuxtjs/seo": "^3.1.0",
    "@nuxtjs/sitemap": "^7.4.3",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@openapitools/openapi-generator-cli": "^2.23.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.5",
    "@semantic-release/npm": "^12.0.2",
    "@types/jsdom": "^27.0.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vite-pwa/nuxt": "^1.0.4",
    "@vue/compiler-dom": "3.5.22",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/nuxt": "^13.3.0",
    "cssnano": "^7.1.1",
    "dayjs-nuxt": "^2.1.11",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.4.0",
    "happy-dom": "^19.0.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-prefix-selector": "^2.1.1",
    "prettier": "^3.6.2",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.92.1",
    "semantic-release": "^24.2.7",
    "vite-plugin-vuetify": "^2.1.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.6",
    "vuetify": "^3.9.7"
  },
  "packageManager": "pnpm@10.18.2",
  "engines": {
    "node": ">=20.x"
  },
  "pnpm": {
    "overrides": {
      "vue": "3.5.22"
    },
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@openapitools/openapi-generator-cli",
      "@parcel/watcher",
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  },
  "resolutions": {
    "vue": "3.5.22"
  },
  "overrides": {
    "glob": "^11.0.0"
  }
}
