{
  "name": "nudger-nuxt-front",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "pnpm lint --fix && nuxt build",
    "build:ssr": "pnpm lint --fix && NODE_OPTIONS='--max-old-space-size=8192' nuxt build",
    "preview": "nuxt preview",
    "generate": "nuxt generate",
    "lint": "eslint .",
    "format": "prettier --check .",
    "test": "vitest",
    "storybook": "pnpm exec storybook dev -p 6006 --no-open",
    "storybook:build": "pnpm exec storybook build",
    "generate:api": "openapi-generator-cli generate -i https://beta.front-api.nudger.fr/v3/api-docs/front -g typescript-fetch -o src/api --skip-validate-spec",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.5.0",
    "@nuxt/eslint": "^1.4.1",
    "@nuxt/eslint-config": "^1.4.1",
    "@nuxt/image": "^1.10.0",
    "@nuxt/postcss8": "^1.1.3",
    "@nuxt/ui": "^3.1.3",
    "@nuxtjs/i18n": "^9.5.6",
    "@nuxtjs/plausible": "^1.2.0",
    "@nuxtjs/robots": "^5.2.11",
    "@nuxtjs/seo": "^3.0.3",
    "@nuxtjs/sitemap": "^7.4.3",
    "@nuxtjs/strapi": "^2.0.0",
    "@nuxtjs/tailwindcss": "7.0.0-beta.0",
    "@pinia/nuxt": "^0.11.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.1",
    "@storybook-vue/nuxt": "^8.3.5",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/vue3": "^8.6.14",
    "@tailwindcss/postcss": "^4.1.10",
    "@types/node": "^20",
    "@tsconfig/node18": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vite-pwa/nuxt": "^1.0.4",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/nuxt": "^13.3.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "nuxt": "^3.17.5",
    "@nuxt/schema": "^3.17.5",
    "@openapitools/openapi-generator-cli": "^2.21.0",
    "pinia": "^3.0.3",
    "prettier": "^3.5.3",
    "semantic-release": "^24.2.5",
    "storybook": "8.6.14",
    "tailwindcss": "4.1.10",
    "vitest": "^3.2.3",
    "vue": "^3.5.16",
    "vue-demi": "^0.14.10"
  },
  "packageManager": "pnpm@8.15.9",
  "dependencies": {
    "vue-lazy-hydration": "2.0.0-beta.4"
  }
}
